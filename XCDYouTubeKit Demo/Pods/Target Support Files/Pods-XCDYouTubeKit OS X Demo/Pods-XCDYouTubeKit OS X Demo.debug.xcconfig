GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1 ${inherited} NSLOGGER_WAS_HERE=1 NSLOGGER_BUILD_USERNAME="${USER}"
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/CocoaLumberjack" "${PODS_ROOT}/Headers/Public/NSLogger" "${PODS_ROOT}/Headers/Public/XCDLumberjackNSLogger"
LIBRARY_SEARCH_PATHS = $(inherited) "$CONFIGURATION_BUILD_DIR/CocoaLumberjack-OSX" "$CONFIGURATION_BUILD_DIR/NSLogger-OSX" "$CONFIGURATION_BUILD_DIR/XCDLumberjackNSLogger-OSX"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/CocoaLumberjack" -isystem "${PODS_ROOT}/Headers/Public/NSLogger" -isystem "${PODS_ROOT}/Headers/Public/XCDLumberjackNSLogger"
OTHER_LDFLAGS = $(inherited) -ObjC -l"CocoaLumberjack-OSX" -l"NSLogger-OSX" -l"XCDLumberjackNSLogger-OSX" -framework "CFNetwork" -framework "CoreServices" -framework "SystemConfiguration"
PODS_ROOT = ${SRCROOT}/Pods
