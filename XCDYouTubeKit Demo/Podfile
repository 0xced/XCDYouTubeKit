xcodeproj 'XCDYouTubeKit Demo.xcodeproj'
workspace '../XCDYouTubeKit Demo.xcworkspace'

def import_pods
  pod 'XCDLumberjackNSLogger', '~> 1.0.0'
end

target 'XCDYouTubeKit iOS Demo' do
  platform :ios, '5.0'
  import_pods
end

target 'XCDYouTubeKit OS X Demo' do
  platform :osx, '10.9'
  import_pods
end
